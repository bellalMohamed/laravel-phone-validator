{
    "name": "bellal-mohamed/laravel-phone-validator",
    "description": "A phone validator for Laravel using the free Twilio phone lookup service.",
    "homepage": "https://github.com/BellalMohamed/laravel-phone",
    "license": "MIT",
    "keywords": [
        "laravel",
        "phone",
        "twilio",
        "validator",
        "validation",
        "format",
        "formatter"
    ],
    "authors": [
        {
            "name": "Andrew Ellis",
            "email": "awellis89@gmail.com",
            "homepage": "https://www.ellisthedev.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "twilio/sdk": "~5.34.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0|^5.0",
        "mockery/mockery": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bellal\\Phone\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bellal\\Tests\\Phone\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bellal\\Phone\\PhoneServiceProvider"
            ],
            "aliases": {
                "Phone": "Bellal\\Phone\\Facades\\Phone"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
